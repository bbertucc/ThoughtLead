<div id="header">
  <img src="" alt="" />

  <h1 class="community_name"><%= link_to current_community, community_home_url %></h1>


  <ul class="navigation_top">
    <li class="current"><%= link_to "Learn", courses_url %></li>
    <li><%= link_to "Discuss", discussions_url %></li>
    <li><%= link_to "Connect", users_url %></li>
  </ul>

  <div class="user_controls">
    <% if logged_in? %>
      <%= link_to("My Profile", user_url(current_user)) %> &nbsp;|&nbsp; 
  	
    	<% if current_user.owner? %>
    	  <%= link_to "Community Setup", edit_community_url %>
    	  &nbsp;|&nbsp;
    	<% end %>

    	<%= link_to('Logout', logout_url) %>
  	
    
    <% else %>
    	<%= link_to('Login', login_url) %> | 
    	<%= link_to "Signup", signup_url %>
    <% end %>
  </div>
</div>                  

