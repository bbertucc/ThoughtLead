<h3>User Profiles: Showing <%= pluralize(@users.size, 'Result') %></h3>
<ul>
<% @users.each do |user| %>
  <li>
	  Name: <%= link_to user.display_name, user %> <br/>
	  About: <%= user.about -%> <br/>
	  Interests: <%= user.interests -%> <br/>
	  Location: <%= user.location -%> <br/>
	  Zip Code: <%= user.zipcode -%> <br/>
	  <%= user_url(user) %>
  </li>
<% end # @user.each -%>
</ul>